{
    "version": 2,
    "builds": [
        {
            "src": "labs/3/server.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/labs/3/getDate",
            "destination": "/labs/3/server.js"
        },
        {
            "source": "/COMP4537/labs/:path*",
            "destination": "/labs/:path*"
        },
        {
            "source": "/labs/:path*",
            "destination": "/labs/:path*"
        },
        {
            "source": "/",
            "destination": "/index.html"
        }
    ]
}
